{
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "extends": ["plugin:prettier/recommended", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "object-curly-spacing": ["error", "never"]
  }
}
